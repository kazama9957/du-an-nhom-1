@model QLNS.Models.Employee

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Employee Details</h2>
<dl class="dl-horizontal">
    <dt>FullName</dt>
    <dd>@Model.FullName</dd>
    <dt>Gender</dt>
    <dd>@Model.Gender</dd>
    <dt>BirthDate</dt>
    <dd>@Model.BirthDate</dd>
    <dt>Phone</dt>
    <dd>@Model.Phone</dd>
    <dt>Email</dt>
    <dd>@Model.Email</dd>
    <dt>Address</dt>
    <dd>@Model.Address</dd>
    <dt>Position</dt>
    <dd>@Model.Position</dd>
    <dt>DepartmentID</dt>
    <dd>@Model.DepartmentID</dd>
    <dt>Status</dt>
    <dd>@Model.Status</dd>
</dl>
<p>@Html.ActionLink("Back to List", "Index")</p>